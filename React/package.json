{
  "name": "react-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/user-event": "^13.5.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^11.5.0",
    "dotenv": "^16.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.27.0",
    "react-scripts": "^5.0.1",
    "selenium-webdriver": "^4.34.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll",
    "test:unit": "npm test -- --watchAll=false",
    "test:robot": "robot -d src/selenium/output src/selenium/",
    "test:allLocal": "start /b cmd /c \"npm run start:both > nul\" && timeout 30 && npm run test:unit && npm run test:robot && taskkill /F /IM node.exe > nul",
    "test:all": "npm run test && npm run test:robot",
    "eject": "react-scripts eject",
    "server": "nodemon server.js",
    "start:both": "concurrently \"react-scripts start\" \"nodemon server.js\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jest-fetch-mock": "^3.0.3",
    "jspdf": "^2.5.2",
    "mongoose": "^8.16.5",
    "mysql": "^2.18.1",
    "mysql2": "^3.11.5",
    "nodemon": "^3.1.10"
  }
}
